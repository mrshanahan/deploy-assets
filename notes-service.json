{
    "src": {
        "type": "local",
        "name": "local",
        "path": "."
    },
    "dst": {
        "type": "ssh",
        "name": "quemot.dev",
        "server": "quemot.dev:22",
        "username": "{{ SSH_USERNAME }}",
        "key_file": "{{ SSH_KEY_FILE_PATH }}",
        "run_elevated": true
    },
    "transport": {
        "type": "s3",
        "bucket_url": "s3://quemot-dev-bucket"
    },
    "assets": [
        {
            "type": "dir",
            "src_path": "package-files",
            "dst_path": "~/package"
        },
        {
            "type": "docker-image",
            "repository": [
                "notes-api/auth",
                "notes-api/auth-db",
                "notes-api/auth-cli",
                "notes-api/api",
                "notes-api/web"
            ]
        }
    ]
}